processes:
  perm:
    executable: /var/vcap/packages/perm/bin/perm

    args:
    - --listen-hostname
    - <%= p('listen_hostname') %>
    - --listen-port
    - <%= p('listen_port') %>
    - --log-level
    - <%= p('log_level') %>

    env: []

    limits:
      memory: 256M
      processes: 10
      open_files: 100

    volumes: []

    hooks: {}
